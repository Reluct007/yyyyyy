{
  "name": "labubu-project",
  "version": "1.0.0",
  "private": true,
  "description": "Labubu Wholesale - Multi-theme website project",
  "scripts": {
    "dev": "npm run dev --prefix themes/labubu",
    "build": "npm run build --prefix themes/labubu",
    "start": "npm run start --prefix themes/labubu",
    "lint": "npm run lint --prefix themes/labubu",
    "pages:build": "npm run pages:build --prefix themes/labubu",
    "theme:install": "cd themes/labubu && npm install",
    "theme:list": "node -e \"const c=require('./themes/theme.config.js');console.log('Active:',c.activeTheme);console.log('Available:',c.availableThemes.map(t=>t.name).join(', '))\""
  },
  "keywords": ["labubu", "wholesale", "themes", "next.js"],
  "author": "",
  "license": "UNLICENSED"
}
