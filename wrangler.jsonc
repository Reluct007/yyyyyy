{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "yyyyyy-production",
    "main": ".open-next/worker.js",
    "compatibility_date": "2024-12-30",
    "compatibility_flags": [
        "nodejs_compat"
    ],
    "assets": {
        "directory": ".open-next/assets",
        "binding": "ASSETS"
    },
    "r2_buckets": [
        {
            "binding": "UPLOADS_BUCKET",
            "bucket_name": "yyyyyy-uploads"
        }
    ]
}